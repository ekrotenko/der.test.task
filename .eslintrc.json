{
  "extends": "standard",
  "globals": {
    "ptorHelper": true,
    "projectDir": true
  },
  "env": {
    "browser": true,
    "node": true,
    "protractor": true,
    "jasmine": true
  },
  "rules":{
    "max-line-length": [true, 120],
    "semi": [2, "always"],
    "no-return-await": [0],
    "space-before-function-paren": [0]
  }
}
